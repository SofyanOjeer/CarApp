<?xml version="1.0" encoding="utf-8"?>
    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">


    <TextView
            android:id="@+id/name_page"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="34sp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.096"
            android:text="Menu Principal"/>
    <Button
            android:id="@+id/button1"
            android:layout_width="120dp"
            android:layout_height="70dp"
            android:text="Saisie des Véhicules"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.312" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.234"/>


    <Button
            android:id="@+id/button2"
            android:layout_width="120dp"
            android:layout_height="70dp"
            android:text="Liste des Véhicules"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.765" app:layout_constraintVertical_bias="0.312"/>
    <Button
            android:id="@+id/button3"
            android:layout_width="120dp"
            android:layout_height="70dp"
            android:text="Saisie des Interventions"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.234" app:layout_constraintVertical_bias="0.499"/>
    <Button
            android:id="@+id/button4"
            android:layout_width="120dp"
            android:layout_height="70dp"
            android:text="Liste des Interventions"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.765" app:layout_constraintVertical_bias="0.499"/>
    <Button
            android:id="@+id/button5"
            android:layout_width="120dp"
            android:layout_height="70dp"
            android:text="Saisie des propriétaires"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.234" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.654"/>

    <Button
            android:id="@+id/button6"
            android:layout_width="120dp"
            android:layout_height="70dp"
            android:text="Liste des propriétaires"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.765" app:layout_constraintVertical_bias="0.654"/>
    <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="89dp"
            android:layout_height="69dp"
            app:srcCompat="@android:drawable/ic_lock_power_off"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.842"/>


</androidx.constraintlayout.widget.ConstraintLayout>